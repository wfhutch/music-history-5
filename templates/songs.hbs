<div class="songs">
  {{#each songs}}
  <div data-key={{key}}>
    <h2>{{name}}<span class='glyphicon glyphicon-music music-notes' aria-hidden='true'></span></h2>
    <p class="songinfo">By {{artist}} on the album {{album}}</p>
    <button id="{{key}}" type='button' class='btn btn-danger btn-xs deleteButton'>Remove</button>
  </div>
  {{/each}}
</div>